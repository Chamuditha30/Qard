{
  "expo": {
    "name": "Qard",
    "slug": "Qard",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/qardIcon.png",
    "scheme": "qard",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/qardIconForeground.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.qard.qard"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/qardIcon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splashIcon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "039c8a75-d01c-4f16-b3d1-f5da19554933"
      }
    }
  }
}
